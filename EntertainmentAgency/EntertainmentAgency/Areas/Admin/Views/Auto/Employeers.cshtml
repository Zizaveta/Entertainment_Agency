@model List<EntertainmentAgency.Models.Employeer>
@{
    ViewBag.Title = "Employeers";
}

<div class="text-center">
    @if (Model != null)
    {
        <table class="table table-striped">
            <caption><h1>Employeers</h1></caption>
            <tr>
                <th>Email</th>
                <th>Name</th>
                <th>Phone</th>
            </tr>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>@Model[i].Email</td>
                    <td>@Model[i].Name</td>
                    <td>@Model[i].Phone</td>
                </tr>
            }
        </table>
    }
    else
    {
        <h1>Empty menu!!!</h1>
    }
</div>