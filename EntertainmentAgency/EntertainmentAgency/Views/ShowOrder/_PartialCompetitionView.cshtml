@model List<EntertainmentAgency.Models.Competition>
<div class="text-center">
    <h1>Competitions</h1>
@if (Model != null)
{
    <ul class="list-group col-3">
        @for(int i=0; i<Model.Count; i++)
        {
            <li class="list-group-item">@Model[i].Name</li>
        }
    </ul>
}
else
{
    <h1>Null Competitions</h1>
}
</div>